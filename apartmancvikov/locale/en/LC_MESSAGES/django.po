#: apartmancvikov/templates/index.html:132
msgid "Venkovní bazén"
msgstr "Outdoor swimming pool"

#: apartmancvikov/templates/index.html:137
msgid "Pumptrack a hřiště"
msgstr "Pumptrack and playground"

#: apartmancvikov/templates/index.html:142
msgid "Lezecká stěna se skluzavkou a pískovištěm"
msgstr "Climbing wall with slide and sandpit"

#: apartmancvikov/templates/index.html:147
msgid "Houpačka se skluzavkou a vrbovým tunelem"
msgstr "Swing with slide and willow tunnel"

#: apartmancvikov/templates/index.html:152
msgid "Blátivá kuchyňka"
msgstr "Mud kitchen"

#: apartmancvikov/templates/index.html:157
msgid "Trampolína a prolézačky"
msgstr "Trampoline and climbing frame"

#: apartmancvikov/templates/index.html:162
msgid "Terasa"
